{
    "name": "greatsun-dev",
    "build": {
      "dockerfile": "Dockerfile",
      "args": {
        "VARIANT": "3.11-bullseye"
      }
    },
    "features": {
      },
    "customizations": {
      "vscode": {
        "extensions": [
        ],
        "settings": {
        }
      }
    },
    "postCreateCommand": "echo 'export PATH=$PATH:/workspaces/greatsun-dev/avatar' >> ~/.bashrc",
